<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             	xmlns:ve="clr-namespace:LibraryTestAppMaui;assembly=LibraryTestAppMaui"

             x:Class="LibraryTestAppMaui.MainPage">

           <ContentPage.Resources>
        <ResourceDictionary>
             <Style x:Key="InfoAlertTitle" TargetType="Label">
                <Setter Property="HorizontalOptions">Center</Setter>
                <Setter Property="HorizontalTextAlignment">Center</Setter>
                <Setter Property="ve:TypographyEffect.LineHeight" Value="30" />
                <Setter Property="ve:TypographyEffect.FontWeight" Value="Bold" />
                <Setter Property="TextColor" Value="Black" />
            </Style>
        </ResourceDictionary>   
    </ContentPage.Resources>

    
              
     
    <StackLayout>
            <Label Padding="10,100,10,0" Text="Welcome to Xamarin.Forms!" Style="{StaticResource InfoAlertTitle}" HorizontalTextAlignment="Center"/>
       


    </StackLayout>

</ContentPage>

